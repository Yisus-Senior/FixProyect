			style="-fx-background-color: rgb(65, 169, 213);"
			text="Formulario estudiantes">
			<font>
				<Font name="Times New Roman Bold" size="24.0" />
			</font>
		</Label>
		<TextField fx:id="tfCarnet" layoutX="30.0" layoutY="55.0"
			prefHeight="25.0" prefWidth="280.0" promptText="Ingrese el carnet" />
		<Label layoutX="30.0" layoutY="36.0" text="Carnet" />
		<TextField fx:id="tfName" layoutX="30.0" layoutY="105.0"
			prefHeight="26.0" prefWidth="280.0" promptText="Ingrese el nombre">
			<font>
				<Font name="System Italic" size="12.0" />
			</font>
		</TextField>
		<Label layoutX="30.0" layoutY="87.0" text="Nombre" />
