			prefHeight="26.0" prefWidth="280.0" promptText="porejemplo@mail.com">
			<font>
				<Font name="System Italic" size="12.0" />
			</font>
		</TextField>
		<Label layoutX="30.0" layoutY="144.0" text="Correo electronico" />
		<TextField fx:id="tfPhoneNumber" layoutX="30.0"
			layoutY="214.0" prefHeight="26.0" prefWidth="280.0"
			promptText="Ingrese su numero de telefono">
			<font>
				<Font name="System Italic" size="12.0" />
			</font>
		</TextField>
		<Label layoutX="30.0" layoutY="196.0" text="Telefono" />
		<Label layoutX="30.0" layoutY="247.0" text="Genero" />
		<TextField fx:id="tfAvailableMoney" layoutX="30.0"
			layoutY="319.0" prefHeight="26.0" prefWidth="280.0"
