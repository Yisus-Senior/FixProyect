			<font>
				<Font name="System Italic" size="12.0" />
			</font>
		</TextField>
		<Label layoutX="30.0" layoutY="301.0" text="Dinero disponible" />
		<Label fx:id="labelAlert" layoutX="30.0" layoutY="381.0"
			prefHeight="57.0" prefWidth="280.0" stylesheets="@stylesForms.css"
			textAlignment="CENTER" textFill="#ff2200" wrapText="true">
			<font>
				<Font size="11.0" />
			</font>
		</Label>
		<Button fx:id="btnBack" layoutX="30.0" layoutY="449.0"
			mnemonicParsing="false" onAction="#back" prefHeight="10.0"
			prefWidth="146.0" text="Regresar" textFill="WHITE" />
		<Button fx:id="btnRegisterStudent" layoutX="30.0"
			layoutY="358.0" mnemonicParsing="false" onAction="#registerStudent"
			prefHeight="23.0" prefWidth="280.0" text="Registrar" textFill="WHITE" />
